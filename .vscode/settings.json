{
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vendor/**/{Tests,tests}/**",
        "**/.history/**",
        "**/vendor/**/vendor/**",
        "**/vendor/_laravel_ide/_request.php"
    ],
    "intelephense.environment.includePaths": [
        "./vendor/pestphp",
        "./vendor/phpunit/phpunit",
        "./vendor/laravel"
    ],
    "intelephense.files.maxSize": 10000000,
    "php.version": "8.4.2",
    "tailwindCSS.classFunctions": [
        "tw",
        "clsx",
        "cn"
    ],
    "tailwindCSS.experimental.configFile": {
        "resources/js/admin/css/admin.css": "resources/js/admin/**",
    }
}